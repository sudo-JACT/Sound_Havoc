@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.

<https://github.com/sudo-JACT/Sound_Havoc/tree/main/Sound_Havoc_Fuzz/src> a lv2:Plugin , lv2:DistortionPlugin , doap:Project;
    lv2:binary <Sound_Havoc_Fuzz.so>;
    doap:name "Sound Havoc Fuzz";
    lv2:optionalFeature lv2:hardRTCapable;
    lv2:minorVersion 1;
    lv2:microVersion 2;

    lv2:port [

            a lv2:InputPort , lv2:AudioPort;
            lv2:index 0;
            lv2:symbol "audio_in";
            lv2:name "Audio input";
        ], 

        [
            a lv2:OutputPort , lv2:AudioPort;
            lv2:index 1;
            lv2:symbol "audio_out";
            lv2:name "Audio output";
        ] ,

        [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 2;
            lv2:symbol "dist";
            lv2:name "Distortion";
            lv2:default 5.0;
            lv2:minimum 1.0;
            lv2:maximum 10.0;
        ] ,

        [
            a lv2:InputPort , lv2:ControlPort;
            lv2:index 3;
            lv2:symbol "quick_adjust" ;
            lv2:name "Quick Adjust" ;
            lv2:default 0;
            lv2:minimum 0;
            lv2:maximum 1;

            lv2:portProperty lv2:integer, pprop:trigger, lv2:toggled;
        ] .